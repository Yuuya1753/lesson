$("#like_" + <%= @likes.tweet_id %>).html('<%= form_with(model: Like.new) do |form| %><div class="field"><%= form.hidden_field :tweet_id, value: @likes.tweet_id %></div><div class="field"><%= form.hidden_field :user_id, value: current_user.id %></div><div class="actions"><%= form.submit(['like']) %><% end %></div><%= Like.where('tweet_id = ?', @likes.tweet_id).pluck(:user_id).size %>')